{
  "name": "content-moderation-api",
  "version": "1.0.0",
  "description": "AI-powered content moderation API using Moondream vision model",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "docker:build": "docker build -t content-moderation .",
    "docker:run": "docker run -p 3000:3000 content-moderation"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2"
  },
  "keywords": ["content-moderation", "ai", "llm", "docker"],
  "author": "",
  "license": "MIT"
}